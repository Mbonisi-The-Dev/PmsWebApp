@model PmsWebApi.Models.TenantLeaseRenewal

@{
    ViewData["Title"] = "Create";
}

<h1>Create Renewal For </h1>

@Html.DisplayFor(m => m.LeaseNumber)


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <table>
        <thead></thead>
        <tbody>

            <tr>
                <td> Renew Number </td>
                <td>
                    @Html.EditorFor(m => m.RenewNumber, new { htmlAttributes = new { @class = "form-control" } })
                </td>
            </tr>
           
            <tr>
                <td> Lease Number </td>
                <td>
                    @Html.EditorFor(m => m.LeaseNumber, new { htmlAttributes = new { @class = "form-control" } })
                </td>
            </tr>

            <tr>
                <td> Notes </td>
                <td>
                    @Html.EditorFor(m => m.AddNotes, new { htmlAttributes = new { @class = "form-control" } })
                </td>
            </tr>

        </tbody>
    </table>

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">

            <input type="submit" value="Create" class="btn btn-primary btn-group-lg" />


        </div>
    </div>
}



