@model PmsWebApi.Models.Maintenance
@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <table>
        <thead></thead>
        <tbody>
            <tr>
                <td> User Id </td>
                <td>
                    @Html.DisplayFor(m => m.LogId, new { htmlAttributes = new { @class = "form-control" } })
                </td>
            </tr>

            <tr>
                <td> Call Status </td>
                <td>
                    @Html.DisplayFor(m => m.CallStatus, new { htmlAttributes = new { @class = "form-control" } })
                </td>
            </tr>

            <tr>
                <td> Call Description </td>
                <td>
                    @Html.DisplayFor(m => m.CallDescription, new { htmlAttributes = new { @class = "form-control" } })
                </td>
            </tr>



            <tr>
                <td> Id Number </td>
                <td>
                    @Html.DisplayFor(m => m.IdNumber, new { htmlAttributes = new { @class = "form-control" } })
                </td>
            </tr>

        </tbody>
    </table>

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">

            <input type="submit" value="Delete" class="btn btn-primary btn-group-lg" />


        </div>
    </div>
}