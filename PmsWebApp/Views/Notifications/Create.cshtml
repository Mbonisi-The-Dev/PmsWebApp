@model PmsWebApi.Models.Notifications
@{
    ViewData["Title"] = "Create";
}

<h1> Create A Notification </h1>

@{
    ViewData["Title"] = "Create";
}



@Html.DisplayFor(m => m.NoteId)


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <table>
        <thead></thead>
        <tbody>


            <tr>
                <td> Note Id </td>
                <td>
                    @Html.EditorFor(m => m.NoteId, new { htmlAttributes = new { @class = "form-control" } })
                </td>
            </tr>




            <tr>
                <td> Note Description   </td>
                <td>
                    @Html.EditorFor(m => m.NoteDescription, new { htmlAttributes = new { @class = "form-control" } })
                </td>
            </tr>

        </tbody>
    </table>

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">

            <input type="submit" value="Create" class="btn btn-primary btn-group-lg" />


        </div>
    </div>
}





